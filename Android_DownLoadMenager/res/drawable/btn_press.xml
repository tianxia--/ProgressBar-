<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true"><layer-list>
            <item android:right="1dp" android:top=" 1dp"><layer-list>
                   
                    <item><shape android:shape="oval">
                            <solid android:color="#11000000" />

                            <padding android:bottom="1dp" android:left="1dp" android:right="1dp" android:top="1dp" />
                        </shape></item>
                    <item><shape android:shape="oval">
                            <solid android:color="#12000000" />

                            <padding android:bottom="1dp" android:left="1dp" android:right="1dp" android:top="1dp" />
                        </shape></item>
                    <item><shape android:shape="oval">
                            <solid android:color="#13000000" />

                            <padding android:bottom="1dp" android:left="1dp" android:right="1dp" android:top="1dp" />
                        </shape></item>
                    <item><shape android:shape="oval">
                            <solid android:color="#14000000" />

                            <padding android:bottom="1dp" android:left="1dp" android:right="1dp" android:top="1dp" />
                        </shape></item>
                    <item><shape android:shape="oval">
                            <solid android:color="#15000000" />

                            <padding android:bottom="1dp" android:left="1dp" android:right="1dp" android:top="1dp" />
                        </shape></item>
                    <item><shape android:shape="oval">
                            <solid android:color="#16000000" />

                            <padding android:bottom="1dp" android:left="1dp" android:right="1dp" android:top="1dp" />
                        </shape></item>
                </layer-list></item>
                <item >
                    <shape android:shape="oval">
                        <solid android:color="#90caf9"/>
                    </shape>
                </item>
        </layer-list></item>

    <item android:state_enabled="true">
        <layer-list >
            <item android:top="2dp" android:right="1dp">
                <layer-list >
                    
                    <item>
                        <shape android:shape="oval">
                            <solid android:color="#11000000"/>
                            <padding android:left="1dp"
                                android:top="1dp"
                                android:right="1dp"
                                android:bottom="1dp"/>
                        </shape>
                    </item>
                    <item>
                        <shape android:shape="oval">
                            <solid android:color="#12000000"/>
                            <padding android:left="1dp"
                                android:top="1dp"
                                android:right="1dp"
                                android:bottom="1dp"/>
                        </shape>
                    </item>
                    <item>
                        <shape android:shape="oval">
                            <solid android:color="#13000000"/>
                            <padding android:left="1dp"
                                android:top="1dp"
                                android:right="1dp"
                                android:bottom="1dp"/>
                        </shape>
                    </item>
                    <item>
                        <shape android:shape="oval">
                            <solid android:color="#14000000"/>
                            <padding android:left="1dp"
                                android:top="1dp"
                                android:right="1dp"
                                android:bottom="1dp"/>
                        </shape>
                    </item>
                    <item>
                        <shape android:shape="oval">
                            <solid android:color="#15000000"/>
                            <padding android:left="1dp"
                                android:top="1dp"
                                android:right="1dp"
                                android:bottom="1dp"/>
                        </shape>
                    </item>
                    <item>
                        <shape android:shape="oval">
                            <solid android:color="#16000000"/>
                            <padding android:left="1dp"
                                android:top="1dp"
                                android:right="1dp"
                                android:bottom="1dp"/>
                        </shape>
                    </item>
                </layer-list>
            </item>
            <item >
                <shape android:shape="oval">
                    <solid android:color="#03A9f4"/>
                </shape>
            </item>
        </layer-list>
    </item>
</selector>